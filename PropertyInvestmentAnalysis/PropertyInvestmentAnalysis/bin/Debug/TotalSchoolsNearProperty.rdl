  </DataSources>
  <DataSets>
    <DataSet Name="DSetSchoolCount">
      <Query>
        <DataSourceName>DSSchoolCount</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@propertyid">
            <Value>=Parameters!propertyid.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>select S.SchoolTypeCommon,count(S.SchoolId) NumberOfSchool from DimSchoolInfo S 
inner join DimPropertySchool PS on PS.DWSchoolKey=S.DWSchoolKey
inner join DimProperty p on P.DWPropertyKey=PS.DWPropertyKey
where P.PropertyId=@propertyid
group by S.SchoolTypeCommon
order by 1</CommandText>
      </Query>
      <Fields>
        <Field Name="SchoolTypeCommon">
          <DataField>SchoolTypeCommon</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="NumberOfSchool">
          <DataField>NumberOfSchool</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
  </DataSets>
